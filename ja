prototype.ts
function student(){
this.name='john';
this.gender='m,;
}
student.prototype.age=18;
var s1=new student();
console.log(s1.age);
var s2=new student();
console.log(s1.name);
var s3=new student();
console.log(s1.gender);



arrow function
const add=(a,b)=>{
return a+b;
}
const add=(a,b)=>a+b;
const message=()=>"Hello World";
